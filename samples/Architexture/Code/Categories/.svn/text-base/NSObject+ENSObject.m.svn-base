#import "NSObject+ENSObject.h"

@implementation NSObject (ENSObject)

@dynamic mainAppDelegate;
@dynamic storyboard;

- (AppDelegate*) mainAppDelegate
{
    return [AppDelegate mainAppDelegate];
}

- (UIStoryboard*) storyboard
{
    return [UIStoryboard storyboardWithName:@"Storyboard" bundle:nil];
}

@end